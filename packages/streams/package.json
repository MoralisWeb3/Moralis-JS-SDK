{
  "name": "@moralisweb3/streams",
  "author": "Moralis",
  "version": "2.11.1",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "moralis": {
    "name": "Streams",
    "type": "feature",
    "scope": "server",
    "description": "Subscribe to realtime data from the blockchain"
  },
  "files": [
    "lib/*"
  ],
  "scripts": {
    "test": "yarn jest --runInBand --detectOpenHandles --forceExit --ci",
    "test:coverage": "yarn run test --coverage --coverageReporters json-summary",
    "test:watch": "yarn run test --watch",
    "lint": "eslint . --ext .js,.ts,.tsx,jsx",
    "clean": "rm -rf lib && rm -rf tsconfig.tsbuildinfo && rm -rf tsconfig.build.tsbuildinfo && rm -rf ./node_modules/.cache/nx",
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc --watch"
  },
  "devDependencies": {
    "@moralisweb3/test-utils": "2.11.1",
    "@types/ethereumjs-util": "5.2.0",
    "jest": "29.3.1",
    "openapi-typescript": "^5.2.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@moralisweb3/api-utils": "^2.11.1",
    "@moralisweb3/common-core": "^2.11.1",
    "@moralisweb3/common-evm-utils": "^2.11.1",
    "@moralisweb3/common-streams-utils": "^2.11.1",
    "@moralisweb3/streams-typings": "^1.0.6",
    "ethereumjs-util": "^7.1.0",
    "ethers": "^5.7.1",
    "web3-eth-abi": "^1.8.0"
  }
