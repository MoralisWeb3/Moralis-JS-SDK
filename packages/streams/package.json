{
  "name": "@moralisweb3/streams",
  "author": "Moralis",
  "version": "2.20.0",
  "license": "MIT",
  "type": "module",
  "main": "./lib/esm/index.js",
  "types": "./lib/index.d.ts",
  "typings": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "default": "./lib/index.d.ts"
      },
      "default": {
        "require": "./lib/cjs/index.cjs",
        "default": "./lib/esm/index.js"
      }
    }
  },
  "files": [
    "lib/*"
  ],
  "sideEffects": false,
  "moralis": {
    "name": "Streams",
    "type": "feature",
    "scope": "server",
    "description": "Subscribe to realtime data from the blockchain"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts"
  },
  "scripts": {
    "test": "yarn jest --runInBand --detectOpenHandles --forceExit --ci",
    "test:coverage": "yarn run test --coverage --coverageReporters json-summary",
    "test:watch": "yarn run test --watch",
    "lint": "yarn run -T eslint . --ext .js,.ts,.tsx,jsx",
    "clean": "rm -rf lib && rm -rf ./node_modules/.cache/nx",
    "build": "rollup -c",
    "dev": "tsc --watch"
  },
  "devDependencies": {
    "@moralisweb3/test-utils": "2.20.0",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/ethereumjs-util": "5.2.0",
    "jest": "^29.5.0",
    "openapi-typescript": "^5.2.0",
    "rollup": "^3.10.1",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-typescript2": "^0.34.1"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/sha2": "^5.7.0",
    "@ethersproject/strings": "^5.7.0",
    "@moralisweb3/api-utils": "^2.20.0",
    "@moralisweb3/common-core": "^2.20.0",
    "@moralisweb3/common-evm-utils": "^2.20.0",
    "@moralisweb3/common-streams-utils": "^2.20.0",
    "@moralisweb3/streams-typings": "^1.0.7",
    "ethereumjs-util": "^7.1.0",
    "web3-eth-abi": "^1.8.0"
  }
}
