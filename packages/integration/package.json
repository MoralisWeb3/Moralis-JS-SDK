{
  "name": "test-integration",
  "author": "Moralis",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "main": "./lib/index.js",
  "module": "./lib.esm/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "test": "yarn jest --runInBand --detectOpenHandles --forceExit --no-cache --ci",
    "test:watch": "yarn jest --watch",
    "test:coverage": "yarn jest --coverage",
    "format": "prettier . \"**/*.+(js|ts|json)\" --write",
    "format:check": "prettier . \"**/*.+(js|ts|json)\" --check",
    "lint": "eslint .",
    "clean": "rm -rf lib && rm -rf tsconfig.tsbuildinfo && rm -rf ./node_modules/.cache/nx"
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@moralisweb3/core": "^2.0.0-alpha.3",
    "@moralisweb3/evm-api": "^2.0.0-alpha.3",
    "@moralisweb3/evm-utils": "^2.0.0-alpha.3",
    "eventemitter3": "^4.0.7"
  }
}
