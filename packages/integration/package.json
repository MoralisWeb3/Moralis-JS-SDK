{
  "name": "@moralisweb3/integration",
  "author": "Moralis",
  "version": "2.0.0-alpha.0",
  "license": "MIT",
  "private": true,
  "main": "./lib/index.js",
  "module": "./lib.esm/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules yarn jest --runInBand --detectOpenHandles --forceExit",
    "test:ci": "yarn test --ci",
    "test:old": "yarn jest",
    "test:watch": "yarn jest --watch",
    "test:coverage": "yarn jest --coverage",
    "format": "prettier . '**/*.+(js|ts|json)' --write",
    "format:check": "prettier . '**/*.+(js|ts|json)' --check",
    "lint": "eslint .",
    "clean": "rm -rf lib && rm -rf lib.esm && rm -rf tsconfig.tsbuildinfo"
  },
  "devDependencies": {
    "eslint": "^8.7.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@moralisweb3/core": "^2.0.0-alpha.0",
    "@moralisweb3/server": "^2.0.0-alpha.0",
    "@moralisweb3/evm": "^2.0.0-alpha.0",
    "@moralisweb3/evm-api": "^2.0.0-alpha.0"
  }
}
