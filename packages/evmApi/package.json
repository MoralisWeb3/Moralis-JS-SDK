{
  "name": "@moralisweb3/evm-api",
  "author": "Moralis",
  "version": "2.0.0-alpha.2",
  "license": "MIT",
  "private": false,
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib/index.*",
    "lib/**/*.d.ts",
    "dist"
  ],
  "scripts": {
    "format": "prettier . \"**/*.+(js|ts|json)\" --write",
    "lint": "eslint . --ext .js,.ts,.tsx,jsx",
    "format:check": "prettier . \"**/*.+(js|ts|json)\" --check",
    "clean": "rm -rf lib && rm -rf lib.esm && rm -rf tsconfig.tsbuildinfo",
    "build": "rollup -c rollup.config.js",
    "build:dev": "dev=true rollup -c",
    "gen:types": "openapi-typescript https://deep-index.moralis.io/api-docs/v2/swagger.json --output src/generated/types.ts",
    "dev": "tsc --watch"
  },
  "devDependencies": {
    "openapi-typescript": "^5.2.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@moralisweb3/core": "^2.0.0-alpha.2",
    "ethers": "^5.6.2"
  }
}
