{
  "name": "@moralisweb3/api-utils",
  "author": "Moralis",
  "version": "2.22.4",
  "license": "MIT",
  "type": "module",
  "main": "./lib/esm/index.js",
  "types": "./lib/index.d.ts",
  "typings": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "default": "./lib/index.d.ts"
      },
      "default": {
        "require": "./lib/cjs/index.cjs",
        "default": "./lib/esm/index.js"
      }
    }
  },
  "files": [
    "lib/*"
  ],
  "sideEffects": false,
  "moralis": {
    "name": "Api Utilities",
    "type": "utils",
    "scope": "server",
    "description": "Utilities and types to handle logic for api calls"
  },
  "scripts": {
    "test": "jest --runInBand --detectOpenHandles --forceExit --ci",
    "test:coverage": "yarn run test  --coverage",
    "test:watch": "yarn run test --watch",
    "lint": "yarn run -T eslint . --ext .js,.ts,.tsx,jsx",
    "clean": "rm -rf lib && rm -rf ./node_modules/.cache/nx",
    "build": "rollup -c",
    "dev": "tsc --watch"
  },
  "devDependencies": {
    "@moralisweb3/common-evm-utils": "^2.22.4",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "jest": "^29.5.0",
    "rollup": "^3.10.1",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-typescript2": "^0.34.1"
  },
  "dependencies": {
    "@moralisweb3/common-core": "^2.22.4",
    "@moralisweb3/common-evm-utils": "^2.22.4",
    "axios": "^1.2.1"
  }
}
