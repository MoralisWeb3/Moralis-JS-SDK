  "license": "MIT",
  "private": false,
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib/*"
  ],
  "scripts": {
    "lint": "eslint . --ext .js,.ts,.tsx,jsx",
    "clean": "rm -rf lib && rm -rf tsconfig.tsbuildinfo && rm -rf tsconfig.build.tsbuildinfo && rm -rf ./node_modules/.cache/nx",
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "exports": {
    ".": "./lib/index.js",
    "./core": "./lib/core/index.js",
