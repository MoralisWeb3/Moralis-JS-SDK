{
  "$schema": "https://typedoc.org/schema.json",
  "out": "documentation/docs",
  "media": "./assets",
  "plugin": ["typedoc-plugin-markdown", "@moralisweb3/typedoc-readme-theme"],
  "theme": "readme",
  "entryDocument": "/nodejs-sdk-references.md",
  "entryPoints": ["packages/**"],
  "exclude": ["packages/eslintConfig", "packages/codegen"],
  "entryPointStrategy": "packages",
  "includeVersion": false,
  "logLevel": "Verbose",
  "intentionallyNotExported": [],
  "excludeExternals": true,
  "excludeInternal": true,
  "excludePrivate": true,
  "readme": "none",
  "excludeNotDocumented": false,
  "validation": {
    "notExported": false,
    "invalidLink": true,
    "notDocumented": false
  },
  "disableSources": true,
  "hideGenerator": true,
  "searchInComments": true,
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": false,
    "external": false,
    "@alpha": false,
    "@beta": false
  },
  "sort": ["static-first", "visibility", "alphabetical"]
}
